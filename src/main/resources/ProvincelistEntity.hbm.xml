<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.jxufe.entities.ProvincelistEntity" table="provincelist" schema="CEE_Helper">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(255)"/>
        </property>
        <property name="provinceline">
            <column name="provinceline" sql-type="text"/>
        </property>
    </class>
</hibernate-mapping>