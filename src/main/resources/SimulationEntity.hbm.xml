<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.jxufe.entities.SimulationEntity" table="simulation" schema="CEE_Helper">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(255)"/>
        </property>
        <property name="category">
            <column name="category" sql-type="varchar(255)"/>
        </property>
        <property name="province">
            <column name="province" sql-type="varchar(255)"/>
        </property>
        <property name="link">
            <column name="link" sql-type="varchar(255)"/>
        </property>
        <property name="year">
            <column name="year" sql-type="int(11)"/>
        </property>
        <property name="subject">
            <column name="subject" sql-type="varchar(255)"/>
        </property>
    </class>
</hibernate-mapping>