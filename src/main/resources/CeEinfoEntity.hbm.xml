<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.jxufe.entities.CeEinfoEntity" table="CEEinfo" schema="CEE_Helper">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="title">
            <column name="title" sql-type="varchar(255)"/>
        </property>
        <property name="author">
            <column name="author" sql-type="varchar(255)"/>
        </property>
        <property name="content">
            <column name="content" sql-type="text"/>
        </property>
        <property name="category">
            <column name="category" sql-type="varchar(255)"/>
        </property>
        <property name="province">
            <column name="province" sql-type="varchar(255)"/>
        </property>
        <property name="state">
            <column name="state" sql-type="int(11)"/>
        </property>
        <property name="createtime">
            <column name="createtime" sql-type="date"/>
        </property>
        <property name="updatetime">
            <column name="updatetime" sql-type="date"/>
        </property>
    </class>
</hibernate-mapping>